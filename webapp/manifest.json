{
	"_version": "1.12.0",
	"sap.app": {
		"id": "murphy.mdm.vendor.murphymdmvendor",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"searchVendorMetatDataSource": {
				"uri": "model/SearchVendorMetaData.json",
				"type": "JSON"
			},
			"createVndrDataSource": {
				"uri": "model/localJson.json",
				"type": "JSON"
			},
			"murphyVendor": {
				"uri": "/murphyCustom",
				"type": "OData"
			},
			"changeRequestMetatDataSource": {
				"uri": "model/ChangeRequestMetaData.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"fullWidth": true
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "murphy.mdm.vendor.murphymdmvendor.view.App",
			"type": "XML",
			"async": true,
			"id": "App"
		},
		"dependencies": {
			"minUI5Version": "1.60.38",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "murphy.mdm.vendor.murphymdmvendor.i18n.i18n"
				}
			},
			"CreateVendorModel": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "createVndrDataSource",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"SearchVendorMetadataModel": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "searchVendorMetatDataSource",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"SearchVendorModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"SearchVendorPopupModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"ChangeRequestMetadataModel": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "changeRequestMetatDataSource",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"changeRequestGetAllModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"changeRequestStatisticsModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"CreateERPVendorAddrTableModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"rows": []
				}
			},
			"userManagementModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"role": [],
					"accountGroups": [],
					"data": {}
				}
			},
			"crERPCommentedModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"crERPTitleFilterModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"valueHelps": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"TelCountryCodes": []
				}
			},
			"crERPAttachmentModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"crAuditLogModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			},
			"praAddressModel": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {
					"rows": [],
					"address": {}
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "murphy.mdm.vendor.murphymdmvendor.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "searchvendor",
				"pattern": "",
				"target": [
					"searchvendor"
				]
			}, {
				"name": "createVendor",
				"pattern": "CreateVendor",
				"target": [
					"createVendor"
				]
			}, {
				"name": "erpVendorPrev",
				"pattern": "erpVendorPrev",
				"target": [
					"ERPVendorPreview"
				]
			}],
			"targets": {
				"searchvendor": {
					"viewType": "XML",
					"viewName": "searchvendor"
				},
				"createVendor": {
					"viewType": "XML",
					"viewName": "CreateVendor"
				},
				"CreateERPVendor": {
					"viewType": "XML",
					"viewName": "CreateERPVendor"
				},
				"ERPVendorPreview": {
					"viewType": "XML",
					"viewName": "ERPVendorPreview"
				},
				"ChangeRequest": {
					"viewType": "XML",
					"viewName": "ChangeRequest"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}