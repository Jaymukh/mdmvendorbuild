<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:form="sap.ui.layout.form"
	xmlns:l="sap.ui.layout">
	<form:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="4" labelSpanS="12"
		adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="2" columnsM="2"
		singleContainerFullSize="false">
		<form:content>
			<core:Title text="Relationship"/>
			<Label text="Partner ID"/>
			<Input value="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/parnr}"/>
			<core:Title/>
			<Label text="Partner Description"/>
			<Input value=""/>
			<core:Title text="General Data"/>
			<Label text="Title"/>
			<Select items="{crERPTitleFilterModel>/modelMap}" forceSelection="false"
				selectedKey="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/ANRED}">
				<items>
					<core:Item text="{crERPTitleFilterModel>title_MEDI}" key="{crERPTitleFilterModel>title}"/>
				</items>
			</Select>
			<Label text="Last Name"/>
			<Input value="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/NAME1}" maxLength="35"/>
			<Label text="First Name"/>
			<Input value="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/NAMEV}"/>
			<Label text="Academic Title"/>
			<Select forceSelection="false"
				selectedKey="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/TITEL_AP}">
				<items>
					<core:Item text="Title 1" key="1"/>
					<core:Item text="Title 2" key="2"/>
					<core:Item text="Title 3" key="3"/>
				</items>
			</Select>
			<core:Title/>
			<Label text="Gender/Sex"/>
			<Select forceSelection="false" selectedKey="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/PARGE}">
				<items>
					<core:Item text="Male" key="1"/>
					<core:Item text="Female" key="2"/>
					<core:Item text="Others" key="3"/>
				</items>
			</Select>
			<Label text="Date of Birth"/>
			<Input value="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/GBDAT}"/>
			<Label text="Marital Status"/>
			<Select forceSelection="false" selectedKey="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/FAMST}">
				<items>
					<core:Item text="Single" key="1"/>
					<core:Item text="Married" key="2"/>
					<core:Item text="Widow" key="3"/>
				</items>
			</Select>
			<Label text="Correspondance Language"/>
			<Input value="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/PARLA}" showValueHelp="true"
				valueHelpRequest=".onValueHelpRequested" valueHelpOnly="true">
				<customData>
					<core:CustomData key="title" value="Correspondance Language"></core:CustomData>
					<core:CustomData key="table" value=""></core:CustomData>
					<core:CustomData key="Key1" value="Column 1"></core:CustomData>
					<core:CustomData key="Key2" value="Column 2"></core:CustomData>
					<core:CustomData key="inputKey" value="PARLA"></core:CustomData>
					<core:CustomData key="inputText" value=""></core:CustomData>
				</customData>
			</Input>
			<core:Title text="Contact Person Details"/>
			<Label text="Department"/>
			<Input value="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/ABTNR}"/>
			<Label text="Function"/>
			<Input value="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/PAFKT}"/>
			<Label text="Note"/>
			<Input value="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/PARAU}"/>
			<core:Title/>
			<Label text="Authority"/>
			<Input value="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/PARVO}"/>
			<Label text="VIP"/>
			<Input value="{CreateVendorModel>/createCRVendorData/formData/parentDTO/customData/vnd_knvk/vnd_knvk_1/PAVIP}"/>
		</form:content>
	</form:SimpleForm>
</core:FragmentDefinition>