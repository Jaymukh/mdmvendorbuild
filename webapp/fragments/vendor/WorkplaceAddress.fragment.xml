<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:form="sap.ui.layout.form"
	xmlns:l="sap.ui.layout">
	<form:SimpleForm editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="4" labelSpanS="12"
		adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="3" columnsL="2" columnsM="2"
		singleContainerFullSize="false">
		<form:content>
			<core:Title text="Company"/>
			<Label text="Address"/>
			<Input/>
			<core:Title text="Workplace"/>
			<Label text="Function"/>
			<Input/>
			<Label text="Department"/>
			<Input/>
			<Label text="Room No"/>
			<Input/>
			<Label text="Floor"/>
			<Input/>
			<Label text="Building"/>
			<Input/>
			<core:Title text="Communication"/>
			<Label text="Communication Method"/>
			<Select forceSelection="false" selectedKey="{CreateVendorModel>/changeReq/genData/commMethod}">
				<items>
					<core:Item text="Method 1" key="1"/>
					<core:Item text="Method 2" key="2"/>
					<core:Item text="Method 3" key="3"/>
				</items>
			</Select>
		</form:content>
	</form:SimpleForm>
</core:FragmentDefinition>